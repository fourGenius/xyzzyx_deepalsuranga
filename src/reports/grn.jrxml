	<queryString>
		<![CDATA[SELECT
     grn.`id` AS grn_id,
     grn.`date` AS grn_date,
     grn.`total` AS grn_total,
     grn.`time` AS grn_time,
     grn.`discount` AS grn_discount,
     grn.`nettot` AS grn_nettot,
     grn.`supliername` AS grn_supliername,
     grn.`bal` AS grn_bal,
     grn.`payment` AS grn_payment,
     grnreg.`itemid` AS grnreg_itemid,
     grnreg.`item` AS grnreg_item,
     grnreg.`itemprice` AS grnreg_itemprice,
     grnreg.`qty` AS grnreg_qty,
     grnreg.`subtot` AS grnreg_subtot
